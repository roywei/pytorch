# no groups
--reset
--dir=FWD_D,BWD_D,BWD_W
--cfg=bf16bf16bf16
--batch=shapes_2d

# with groups
--reset
--dir=FWD_D,BWD_D,BWD_W
--cfg=bf16bf16bf16
--stag=abcd
--wtag=acbde
--dtag=abcd
--batch=shapes_2d_groups

# no groups + post-ops
--reset
--dir=FWD_D,FWD_B
--cfg=bf16bf16bf16
--attr-post-ops=add:bf16:common,add:bf16:per_oc,sum,relu
--batch=shapes_2d

# with groups + post-ops
--reset
--dir=FWD_D,FWD_B
--cfg=bf16bf16bf16
--stag=abcd
--wtag=acbde
--dtag=abcd
--attr-post-ops=add:bf16:common,add:bf16:per_oc,sum,relu
--batch=shapes_2d_groups
